{
	"manifest_version": 2,
	"name": "New Tab Back",
	"description": "Re-opens the page from which a new tab was opened.",
	"version": "0.1.0",
	"permissions": ["tabs"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "New Tab Back"
	}
}
